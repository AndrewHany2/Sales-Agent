{
  "name": "chatbot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc -b",
    "start": "node src",
    "clean": "rimraf ./dist",
    "local": "ts-node src/index.ts",
    "dev": "nodemon src -e ts,json --exec \"npm run local\"",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev --name init",
    "prisma:rebuild": "npm run clean && npm run migrate && npm run db:pull && npm run db:push"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.15",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google/genai": "^1.16.0",
    "@prisma/client": "^6.12.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "prisma": "^6.12.0"
  }
}
